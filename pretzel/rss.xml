layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
    <title>Laedit | Jérémie Bertrand</title>
    <link>http://laedit.net/</link>
    <atom:link href="http://laedit.net/rss.xml" rel="self" type="application/rss+xml" />
    <description>TODO</description>
    <language>en-au</language><!-- TODO -->
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title | xml_escape }}</title>
      <link>http://laedit.net{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>Jérémie Bertrand</author>
      <guid>http://laedit.net{{ post.url }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel> 
</rss>