---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Laedit | Jérémie Bertrand</title>
  <link href="http://laedit.net/"/>
  <link type="application/atom+xml" rel="self" href="http://laedit.net/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://laedit.net/</id>
  <author>
    <name>Jérémie Bertrand</name>
    <email>Email</email><!-- TODO -->
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://laedit.net/{{ post.url }}</id>
    <link type="text/html" rel="alternate" href="http://laedit.net/{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Jérémie Bertrand</name>
      <uri>http://laedit.net/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>